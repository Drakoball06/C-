//EJERCICIO 3
Console.WriteLine("Ingrese un número porfavor");
String numero = Console.ReadLine();
int numero1 = Convert.ToInt32(numero);
if(numero1 > 18)
{
    Console.WriteLine("El número es mayor a 18");
}
else
{
    Console.WriteLine("El número es menor a 18");
}

Console.WriteLine("Ingrese una letra");
String letra = Console.ReadLine();
char l = Convert.ToChar(letra);
if( l == 'a')
{
    Console.WriteLine("Hay una a");
}
else
{
    Console.WriteLine("Escribe bien");
}

int zero = 0;
int un = 0;
int deux = 1;
Console.WriteLine( zero == un && zero < deux);
Console.WriteLine(zero > deux || zero == un);

