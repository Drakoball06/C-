Console.WriteLine("Cual es tu nombre? ");
String nombre = Console.ReadLine();
Console.WriteLine("Cual es tu apellido? ");
String apellido = Console.ReadLine();
Console.WriteLine("Que edad tienes? ");
String edad = Console.ReadLine();
int edad1 = Convert.ToInt32(edad);
Console.WriteLine("Sabes programar? ");
String respu = Console.ReadLine();

Console.WriteLine("Bienvenido Usuario: " + nombre + " " + apellido + ", su edad es: " + edad1 + " y usted: " + respu + " sabe programar.");

